                    {"featureType": "landscape.natural.landcover","elementType": "geometry.fill","stylers": [{"color": "#e1e1e1"}]},
                    {"featureType": "landscape.natural.terrain","elementType": "geometry.fill","stylers": [{"color": "#e1e1e1"}]},
