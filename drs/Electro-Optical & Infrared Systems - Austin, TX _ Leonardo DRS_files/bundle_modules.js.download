                    imgUrl = $(slide).data('img-url-lazy-d'); break;
                case 'TABLET_LANDSCAPE':
                    imgUrl = $(slide).data('img-url-lazy-tl'); break;
                case 'TABLET_PORTRAIT':
                    imgUrl = $(slide).data('img-url-lazy-tp'); break;
                case 'MOBILE':
                    imgUrl = $(slide).data('img-url-lazy-m'); break;
