    function getTimelineGridLayout(layout){
        var isotopeObj = {};

        if(layout ==  "fitRows"){
            isotopeObj = {
                itemSelector: '.timeline-articles--gallery-wrap--img',
                layoutMode: layout,
                resize: true,
                fitRows: {
                    gutter: 20
                }
            };
        }else if(layout == "masonry"){
            isotopeObj = {
                itemSelector: '.timeline-articles--gallery-wrap--img',
                layoutMode: layout,
                resize: false,
                masonry: {
                    columnWidth: 20,
                    gutter: 20
                }
            };
        }else if(layout == "masonryHorizontal"){
            isotopeObj = {
                itemSelector: '.timeline-articles--gallery-wrap--img',
                layoutMode: layout,
                resize: true,
                masonryHorizontal: {
                    columnWidth: 20,
                    gutter: 20
                }
            };
        }else if(layout == "fitColumns"){
            isotopeObj = {
                itemSelector: '.timeline-articles--gallery-wrap--img',
                layoutMode: layout,
                resize: true,
